#print a list of fruits
fruits=["banana", "guava", "orange"]
for i in fruits:
   # print(fruits)  
    print(i)   
'''to print fruits in reverse order
for fruits in range (1,3,-1):
    print(i)'''
for fruits in reversed(fruits):
    print(fruits)
#printing in reverse order using slicing method    
for fruit in fruits[::-1]:
    print(fruit)
for fruit in fruits[::-1]:
    print(fruit[::-1])   # THIS reverses each word
    