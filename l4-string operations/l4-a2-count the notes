#count the notes wrong way
amount=int(input("please enter amount to withdraw"))
note100=amount//100
note50=(amount%100)//50
note20=((amount%100)%50)//20
note10=(((amount%100)%50)%20)//10

print("number of 100 notes:",note100)
print("number of 50 notes:",note50)
print("number of 20 notes:",note20)
print("number of 10 notes:",note10)


